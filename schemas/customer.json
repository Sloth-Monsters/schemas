{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "HODLer",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "maxLength": 153
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "dob": {
      "type": "string",
      "format": "date"
    },
    "address": {
      "$ref": "address.json#",
      "type": "object"
    },
    "wallets": {
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "wallet.json#"
      },
      "minItems": 1
    },
    "reviews": {
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "review.json#"
      }
    }
  },
  "required": [

  ]
}